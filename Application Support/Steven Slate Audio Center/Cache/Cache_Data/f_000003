.ini");let m;m=c.existsSync(f)?c.readFileSync(f,"utf-8"):'\n      <?xml version="1.0" encoding="UTF-8"?>\n<PROPERTIES>\n  <VALUE name="SAMPLER_lic_file_path_collection_idx_count" val="0"/>\n  <VALUE name="SamplerBaseDirectory" val=""/>\n</PROPERTIES>\n';const v=yield iH(m);c.existsSync(d)&&c.removeSync(d);let x=ft.find(v.PROPERTIES.VALUE,Me=>"SAMPLER_lic_file_path_collection_idx_count"===Me.$.name);x||(v.PROPERTIES.VALUE.push({$:{name:"SAMPLER_lic_file_path_collection_idx_count",val:0}}),x=ft.last(v.PROPERTIES.VALUE));let z=ft.find(v.PROPERTIES.VALUE,Me=>"SamplerBaseDirectory"===Me.$.name);z||(v.PROPERTIES.VALUE.unshift({$:{name:"SamplerBaseDirectory",val:""}}),z=ft.first(v.PROPERTIES.VALUE)),z.$.val=o.destinationPath,ft.remove(v.PROPERTIES.VALUE,Me=>/SAMPLER_lic_file_path_collection_idx_\d+/.test(Me.$.name));let ue=ft.filter(r.ssd.state.installedProducts,{type:"add-on"});r.currentActiveProduct&&"add-on"===r.currentActiveProduct.type&&(ft.remove(ue,{code:r.currentActiveProduct.code}),ue.push(r.currentActiveProduct)),ue=ft.uniqBy(ue,"code"),x.$.val=0;for(const Me of ue)try{const ze=r.slc.getByProductCode(Me.code),Oe=i$.from(ze.slc_file,"base64");Me.licenseLocation=s.join(o.destinationPath,ze.slc_file_name),c.outputFileSync(Me.licenseLocation,Oe),v.PROPERTIES.VALUE.push({$:{name:`SAMPLER_lic_file_path_collection_idx_${x.$.val}`,val:Me.licenseLocation}}),x.$.val++}catch(ze){console.log(ze)}const Fe=(new dv.Builder).buildObject(v);c.outputFileSync(f,Fe)})()}selectTheFolder(e){var r=this;return Pe(function*(){return new Promise((i,o)=>{r.electron.showOpenDialog({message:e,properties:["openDirectory","createDirectory"]}).then(s=>s.canceled?o():void 0!==s.filePaths&&s.filePaths.length?i(s.filePaths[0]):void console.log(s.filePaths)).catch(s=>(console.log(s),o()))})})()}delay(e){return Pe(function*(){yield new Promise(r=>setTimeout(()=>r(),e))})()}checkFreeSpace(e=null,r,i){var o=this;return Pe(function*(){o.isCheckingSpaceAndPermissions=!0;const c=yield function(){var d=Pe