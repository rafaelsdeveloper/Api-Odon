jQuery(function() {
    
  // Obtener la posiciÃ³n del elemento footer
  var footerTop = document.getElementById('footer').offsetTop;

  // AÃ±adir un evento de scroll al documento
  document.addEventListener('scroll', function() {
    // Si el scroll del usuario ha llegado al elemento footer
    if (window.scrollY + jQuery('.getFixed').outerHeight() + 20 >= footerTop) {
      // Establecer la posiciÃ³n del elemento cache como relativa
      document.querySelector('.getFixed').style.position = 'relative';
    }
  });
    
    if (jQuery(document).outerHeight() > 3300){		
        function fixDiv() {
                var left = jQuery('#sidebar_single').offset().left + jQuery('#main_single').outerWidth() + jQuery('#main_single').offset().left;
                var top = jQuery('.widgete').offset().top + jQuery('.widgete').outerHeight() -40;

                var cache = jQuery('.getFixed');
                if (jQuery(window).scrollTop() > top){
                        jQuery(cache).css({
                        'position': 'fixed',
                        'top': '50px',
                        'left': 'inherit',
                        });
                }
                else{
                    jQuery(cache).css({
                            'position': 'relative',
                            'display':'block',
                            'top': 'auto',
                            'left': 'inherit'

                    });
                }
        console.log(jQuery(document).outerHeight());
       // console.log(left);
        };
    
        jQuery(window).scroll(fixDiv);
        fixDiv();
    }
    
});
    
