OCOM.register(function rc30($){"use strict";var a=getBreakpoint(),b,c=!1,d,e=.75,f="a,input,button,select",g,h,i=40,j=10,k,l=1,m,n=1/60,o,p={},q=250,r,s,t,u,v,w,x;function adjustColorCls(a,b){var c=[],d,e,f=/rw-[^-]+-[0-9]+bg/;$(a).find(".rc30pane > .col2 > .col-w1 > .col-item:first-child").each(function(a,b){c=$(b).get(0).className.split(/\s+/);for(var d=0;d<c.length;d++){e=c[d].match(f);if(e){$(b).removeClass(e[0]);$(b).closest(".rc30pane").addClass(e[0]);break}}})}function buildNav(a,b){var c,d,e=$('<div class="rc30nav"></div>'),f=$(a).find(".rc30mobnav optgroup"),g;if(f[0]){e.addClass("rc30groups");f.each(function(c,d){$(d).attr("label")&&e.append($('<h3 class="rc30grpname">').text($(d).attr("label")));buildNavTabs(a,e,d,c,b)})}else buildNavTabs(a,e,$(a).find(".rc30mobnav"),0,b);if(e.find("li")[0]){$(a).find(".rc30w2").append(e);$(a).find(".rc30nav a").on("click",function(b){b.preventDefault();pushTab(b.currentTarget);navEvent(a,b.currentTarget)})}$(a).on("click",".rc30mobtitle a",function(b){b.preventDefault();navEvent(a,b.currentTarget)});$(a).find(".rc30nav li.rc30active")[0]||navEvent(a,$(a).find(".rc30nav ul:first li:first-child a"));c=$(a).find(".rc30nav li.rc30active a").css("background-color");d=$(a).find(".rc30nav li.rc30active a").css("color");$(a).find(".rc30nav li.rc30active-added").removeClass("rc30active").removeClass("rc30active-added");$(a).find(".rc30mobtitle").attr("style","background-color:"+c+" !important; color:"+d+" !important")}function buildNavTabs(a,b,c,d,e){var f="_"+d,g=$(a).hasClass("rc30v2"),h,i,j=$('<ul role="tablist" aria-orientation="vertical"></ul>'),k;$(j).addClass("rc30grp"+d);b.append(j);$(c).find("option").each(function(j,l){i=$(l).text();k=$(l).attr("value");if(i&&k){h=$('<li role="none"></li>');g?h.append('<a href="'+k+'" id="rc30t'+e+f+"_"+j+'" role="tab" tabindex="-1" aria-selected="false" aria-controls="'+k.replace("#","")+'"><h3><span>'+i+"</span></h3></a>"):h.append('<a href="'+k+'" id="rc30t'+e+f+"_"+j+'" role="tab" t