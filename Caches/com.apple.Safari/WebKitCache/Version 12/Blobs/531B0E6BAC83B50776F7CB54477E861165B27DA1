OCOM.register(function rc140($){var a=document.querySelectorAll(".rc140 div[data-panel]"),b=document.querySelectorAll(".rc140close");function getCurrentContext(a){return a.currentTarget.parentElement.parentElement.parentElement}function isPanelVisible(a){return!!a&&a.classList.contains("rc140active")}function setAriaExpanded(a,b,c){var d=getCurrentContext(a).querySelector("[aria-controls=".concat(b,"]"));d.setAttribute("aria-expanded",c);d.setAttribute("aria-current",c)}function showPanel(a){if(a){a.classList.add("rc140active");return!0}return!1}function hidePanel(a){if(a){a.classList.remove("rc140active");return!0}return!1}function scrollIntoBtnAccordion(a){var b=a.currentTarget.parentElement.parentElement.parentElement.parentElement.parentElement.id;if(window.innerWidth<=650){var c=120,d=a.currentTarget.getBoundingClientRect().top,e=d+window.pageYOffset-120;window.scrollTo({top:e,behavior:"smooth"})}else if(b){var f=document.querySelector("[aria-controls=".concat(b,"]"));f&&f.scrollIntoView({behavior:"smooth",block:"start"})}}function tooglePanel(a,b){var c=getCurrentContext(a).querySelector("#".concat(b));if(isPanelVisible(c)){if(hidePanel(c)){setAriaExpanded(a,b,!1);setInactiveCards(a,b,!0)}}else if(showPanel(c)){scrollIntoBtnAccordion(a);setAriaExpanded(a,b,!0);setInactiveCards(a,b)}}function hideInactivePanels(a,b){getCurrentContext(a).querySelectorAll(".rc140panel.rc140active").forEach(function(c){if(c.id!=b){hidePanel(c);setAriaExpanded(a,c.id,!0);setInactiveCards(a,b)}})}function setInactiveCards(a,b,c){getCurrentContext(a).querySelectorAll(".rc140cards div[data-panel]").forEach(function(a){if(c)a.classList.remove("rc140inactive");else if(a.dataset.panel!==b){a.classList.remove("rc140active");a.classList.add("rc140inactive")}else{a.classList.remove("rc140inactive");a.classList.add("rc140active")}})}function closeExpandedPanel(a,b){a.stopPropagation();hideInactivePanels(a,"");setAriaExpanded(a,b,!1);setInactiveCards(a,b,!0)}function focusCardBtn(a,b){var c=g