A")&&a.target.click();if(!a.target.tagName.match("TEXTAREA|INPUT|SELECT|A"))if(a.keyCode===c.arrowLeft&&!0===b.options.accessibility){b.$dots.find("li").eq(Math.floor(b.currentSlide/b.options.slidesToScroll-1)).find("button");b.changeSlide({data:{message:!0===b.options.rtl?"next":"previous"}})}else if(a.keyCode===c.arrowRight&&!0===b.options.accessibility){b.$dots.find("li").eq(Math.floor(b.currentSlide/b.options.slidesToScroll+1)).find("button");b.changeSlide({data:{message:!0===b.options.rtl?"previous":"next"}})}};a.prototype.lazyLoad=function(){var a=this,b,c,d,e;function loadImages(b){$("img[data-lazy]",b).each(function(){var b=$(this),c=$(this).attr("data-lazy"),d=$(this).attr("data-srcset"),e=$(this).attr("data-sizes")||a.$slider.attr("data-sizes"),f=document.createElement("img");f.onload=function(){b.animate({opacity:0},100,function(){if(d){b.attr("srcset",d);e&&b.attr("sizes",e)}b.attr("src",c).animate({opacity:1},200,function(){b.removeAttr("data-lazy data-srcset data-sizes").removeClass("slick-loading")});a.$slider.trigger("lazyLoaded",[a,b,c])})};f.onerror=function(){b.removeAttr("data-lazy").removeClass("slick-loading").addClass("slick-lazyload-error");a.$slider.trigger("lazyLoadError",[a,b,c])};f.src=c})}if(!0===a.options.centerMode)if(!0===a.options.infinite){d=a.currentSlide+(a.options.slidesToShow/2+1);e=d+a.options.slidesToShow+2}else{d=Math.max(0,a.currentSlide-(a.options.slidesToShow/2+1));e=a.options.slidesToShow/2+1+2+a.currentSlide}else{d=a.options.infinite?a.options.slidesToShow+a.currentSlide:a.currentSlide;e=Math.ceil(d+a.options.slidesToShow);if(!0===a.options.fade){d>0&&d--;e<=a.slideCount&&e++}}b=a.$slider.find(".slick-slide").slice(d,e);if("anticipated"===a.options.lazyLoad)for(var f=d-1,g=e,h=a.$slider.find(".slick-slide"),i=0;i<a.options.slidesToScroll;i++){f<0&&(f=a.slideCount-1);b=b.add(h.eq(f));b=b.add(h.eq(g));f--;g++}loadImages(b);if(a.slideCount<=a.options.slidesToShow){c=a.$slider.find(".slick-slide");loadImages(c)}else if(a.cur