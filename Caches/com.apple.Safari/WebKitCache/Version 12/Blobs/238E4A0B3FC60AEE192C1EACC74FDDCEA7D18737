jQuery(document).ready(function($){"use strict";$(".ocode").each(function(a){var b=$(this),c=a+1,d="ocode-initialized",e="ocode-show",f="showTimeout",g=b.is("code"),h=g?b:b.find("pre > code"),i=b.find(".ocode-bttn").append(function(){return'<div class="ocode-success" aria-hidden="true">'+($(this).data("success")||"Copied to Clipboard")+'</div><div class="ocode-error" aria-hidden="true">'+($(this).data("error")||"Error: Could not Copy")+"</div>"});if(i.find("a")[0]){var j=i.find("a").text();i.first("> div").find("a").remove();i.first("> div").prepend("<button>"+j+"</button>")}i.find("button").attr("aria-label","Copy code block "+c);!g&&b.find("pre")[0]&&b.find("pre").attr("aria-label","Code block "+c);c++;h.html(h.html().trim().replace(/\t/g,"  ").replace(/</g,"&lt;").replace(/>/g,"&gt;"));b.on("keyup",function(a){return 9===a.keyCode&&b.addClass("ocode-kb")});i.on("click",function(){var a=_asyncToGenerator(_regeneratorRuntime().mark(function _callee(a){var c,d,g;return _regeneratorRuntime().wrap(function _callee$(h){for(;;)switch(h.prev=h.next){case 0:c=i.find(".ocode-success"),d=i.find(".ocode-error");a.preventDefault();"function"==typeof arialive&&arialive(c.text());h.next=5;return copyToClipBoard(b);case 5:g=h.sent;if(!g){h.next=10;break}return h.abrupt("return",c.addClass(e).data(f,setTimeout(function(){clearTimeout(c.data(f));c.removeClass(e)},2e3)));case 10:return h.abrupt("return",d.addClass(e).data(f,setTimeout(function(){clearTimeout(d.data(f));d.removeClass(e);"function"==typeof arialive&&arialive(d.text())},2e3)));case 11:case"end":return h.stop()}},_callee)}));return function(b){return a.apply(this,arguments)}}());function copyToClipBoard(a){return _copyToClipBoard.apply(this,arguments)}function _copyToClipBoard(){_copyToClipBoard=_asyncToGenerator(_regeneratorRuntime().mark(function _callee2(a){var b,c;return _regeneratorRuntime().wrap(function _callee2$(d){for(;;)switch(d.prev=d.next){case 0:b=a.is("code");c=b?a:a.find("pre > code");d.prev=2;if(navigat